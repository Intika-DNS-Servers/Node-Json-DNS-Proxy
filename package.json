{
  "name": "phu-dns-proxy",
  "version": "1.0.0",
  "description": "Simple DNS Proxy written in Node.JS. Override hosts, domains, or tlds. Redirect certain domains to different nameservers.",
  "bin": {
    "dns-proxy": "bin/dns-proxy"
  },
  "main": "lib/dns-proxy.js",
  "preferGlobal": true,
  "author": "Erik Kristensen <erik@erikkristensen.com>",
  "license": "MIT",
  "repository": "ekristen/dns-proxy",
  "scripts": {
    "start": "bin/dns-proxy --config conf.json",
    "start-debug": "DEBUG=dnsproxy:debug,dnsproxy:query,dnsproxy:info bin/dns-proxy --config conf.json",
    "test": "npm run test:standard",
    "test:standard": "standard lib/*.js"
  },
  "dependencies": {
    "debug": "^2.6.0",
    "native-dns-packet": "~0.1.1",
    "rc": "^1.1.6"
  },
  "devDependencies": {
    "standard": "^9.0.2"
  }
}
